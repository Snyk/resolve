{
  "name": "snyk-resolve-deps",
  "description": "Resolves a node package tree with combined support for both npm@2 and npm@3.",
  "main": "lib/index.js",
  "directories": {
    "test": "test"
  },
  "bin": {
    "snyk-resolve": "./cli/index.js"
  },
  "scripts": {
    "build": "mkdir -p dist; lodash -p -o ./dist/lodash-min.js include=flatten,extend",
    "lint": "jscs cli/*.js lib/*.js -v",
    "check-tests": "! grep 'test.only' test/*.test.js -n",
    "env": "node -e 'console.log(process.env, process.versions)'",
    "cover": "tap test/*.test.js --cov --coverage-report=lcov",
    "test": "npm run check-tests && npm run lint && tap test/*.test.js --cov --timeout=60",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "author": "Remy Sharp",
  "license": "Apache-2.0",
  "devDependencies": {
    "jscs": "^2.8.0",
    "lodash-cli": "^4.15.0",
    "lodash": "^4.0.0",
    "proxyquire": "^1.7.4",
    "semantic-release": "^4.3.5",
    "sinon": "^1.17.3",
    "snyk-resolve-deps-fixtures": "^1.1.6",
    "tap": "^5.1.1",
    "tap-only": "0.0.5",
    "tape": "^4.4.0"
  },
  "dependencies": {
    "abbrev": "^1.0.7",
    "ansicolors": "^0.3.2",
    "clite": "^0.3.0",
    "debug": "^2.2.0",
    "es6-promise": "^3.0.2",
    "lru-cache": "^4.0.0",
    "minimist": "^1.2.0",
    "semver": "^5.1.0",
    "snyk-module": "^1.6.0",
    "snyk-resolve": "^1.0.0",
    "snyk-tree": "^1.0.0",
    "snyk-try-require": "^1.1.1",
    "then-fs": "^2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Snyk/resolve-deps.git"
  },
  "nyc": {
    "exclude": [
      "dist",
      "test",
      "test{,-*}.js",
      "**/*.test.js",
      "**/__tests__/**"
    ]
  }
}
